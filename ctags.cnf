--langdef=powershell
--langmap=powershell:.ps1.psm1
--regex-powershell=/^[Ff]unction[\t ]*(Global:)?([a-zA-Z0-9_-]+)/\2/f,function/
--regex-powershell=/^[Ff]ilter[\t ]*([a-zA-Z0-9_-]+)/\1/i,filter/
--regex-powershell=/^[sS]et-[Aa]lias[\t ]*([a-zA-Z0-9_-]+)/\1/a,alias/

--langdef=inifile
--langmap=inifile:.ini
--regex-inifile=/^\[([,&0-9A-Za-z \-\.]+)\]/\1/s,section/


